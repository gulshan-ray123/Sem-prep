<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/stylesheets/notes.css">
</head>
<body>
    <div id="first">
        <header id="navbar">
            <ul id="dtl">
                <li><a href="/student/dashboard"><button>Dashboard</button></a></li>
            </ul>
        </header>
    </div>
       <%notesone.forEach(function(user){%>
        <table class="tb" border="2">
        <tr><td>Subject:</td><td><%= user.Subject%></td></tr>
        <tr><td>Year:</td><td><%= user.Year%></td></tr>
        <tr><td>Genre:</td><td><%= user.Genre%></td></tr>
        <tr><td colspan="2"><a href="<%= user.Link%>"><button class="download">Download Now</button></a></td></tr>
    </table>
      <% })%>
</body>
</html>