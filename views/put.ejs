<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/stylesheets/notes.css">
</head>
<body>
    <div id="first">
    <header id="navbar">
        <ul id="dtl">
            <li><a href="/student/dashboard"><button>Dashboard</button></a></li>
        </ul>
    </header>
    <section id="sec">
        <h1 class="yre">Put/Sessional Papers</h1>
        <%paperNew.forEach(function(paper){%>
            <table class="tb" border="2">
                <tr><td>Subject:</td><td><%=paper.Subject %></td></tr>
                <tr><td>Year</td><td><%=paper.Year%></td></tr>
                <tr><td>Genre</td><td><%=paper.Genre%></td></tr>
                <tr><td colspan="2"><a href="<%=paper.Link%>"><button class="download">Download Now</button></a></td></tr>
            </table>
        <% })%>
    </section>
</body>
</html>
    